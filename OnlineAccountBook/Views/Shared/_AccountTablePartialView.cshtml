@model List<OnlineAccountBook.Models.ViewModels.AccountItemViewModels>
@{ int serial = 0;}

<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-hover">
            <tr>
                <th>#</th>
                <th>類別</th>
                <th>日期</th>
                <th>金額</th>
                <th>備註</th>
            </tr>
            @foreach (var data in Model)
            {
                <tr>
                    <td>@(serial++)</td>
                    <td>@data.Category</td>
                    <td>@data.Date.ToString("yyyy-MM-dd")</td>
                    <td>@data.Money.ToString("N0")</td>
                    <td>@data.Description</td>
                </tr>
            }
        </table>
    </div>
</div>
